<!--?xml version="1.0" encoding="UTF-8"?-->
<taglib> 
<tlibversion>1.0</tlibversion>
<jspversion>1.1</jspversion>
<shortname>i18n</shortname>
<info>Sample taglib for internationalization</info>
<uri>http://mhus.de/jsp/taglib/i18n</uri>
<tag>
    <name>i18n</name>
    <tagclass>de.mhus.osgi.tutorial.websampleapp.taglib.I18nTagHandler</tagclass>
    <info>Internationalization</info>
    <attribute>
      <name>key</name>
      <required>true</required>
  </attribute>
</tag>
</taglib> 